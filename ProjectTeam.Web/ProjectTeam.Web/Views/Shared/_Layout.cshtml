<!doctype html>
<html lang="en" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg" data-sidebar-image="none" data-preloader="disable">

<head>

	<meta charset="utf-8" />
	<title>@ViewData["Title"]</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
	<meta content="Themesbrand" name="author" />
	<!-- App favicon -->
	<link rel="shortcut icon" href="~/AdminTemplate/assets/images/favicon.ico">

	<!-- Layout config Js -->
	<script src="~/AdminTemplate/assets/js/layout.js"></script>
	<!-- Bootstrap Css -->
	<link href="~/AdminTemplate/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
	<!-- Icons Css -->
	<link href="~/AdminTemplate/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
	<!-- App Css-->
	<link href="~/AdminTemplate/assets/css/app.min.css" rel="stylesheet" type="text/css" />
	<!-- custom Css-->
	<link href="~/AdminTemplate/assets/css/custom.min.css" rel="stylesheet" type="text/css" />
</head>
<body>
	<!-- Begin page -->
	<div id="layout-wrapper">
		<partial name="_Header" />
		@await Component.InvokeAsync("MainNavBar")
		<div class="main-content">
			<div class="page-content">
				<div class="container-fluid">
					@RenderBody()
				</div>
				<!-- container-fluid -->
			</div>
			<!-- End Page-content -->
		</div>
		<partial name="_Footer" />
	</div>
	<partial name="_BackToTop" />
	<partial name="_Preloader" />
	<partial name="_ThemeSettings" />
	<!-- END layout-wrapper -->
	<!-- JAVASCRIPT -->
	<script src="~/AdminTemplate/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
	<script src="~/AdminTemplate/assets/libs/simplebar/simplebar.min.js"></script>
	<script src="~/AdminTemplate/assets/libs/node-waves/waves.min.js"></script>
	<script src="~/AdminTemplate/assets/libs/feather-icons/feather.min.js"></script>
	<script src="~/AdminTemplate/assets/js/pages/plugins/lord-icon-2.1.0.js"></script>
	<script src="~/AdminTemplate/assets/js/plugins.js"></script>

	<!-- App js -->
	<script src="~/AdminTemplate/assets/js/app.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>

	@await RenderSectionAsync("Scripts", required: false)

</body>

</html>
